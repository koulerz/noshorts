{
  "manifest_version": 3,
  "name": "NoShorts",
  "version": "1.0",
  "description": "NoShorts categorizes your subscribed videos",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["noshorts.js"]
    }
  ]
}
